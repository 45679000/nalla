	<parameter name="sale_no" class="java.lang.String"/>
	<parameter name="broker" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT sale_no, broker, category, DATE_FORMAT(import_date, 'Mon, %b %Y') AS import_date,
(CASE WHEN comment IS NULL THEN '' ELSE comment END) AS comment,
(CASE WHEN rp IS NULL THEN '' ELSE 'RP' END) AS rp,
(CASE WHEN ra IS NULL THEN '' ELSE 'RA' END) AS ra,
(CASE WHEN type IS NULL THEN '' ELSE type END) AS type,
(CASE WHEN standard IS NULL THEN '' ELSE standard END) AS standard,
lot, a.mark, a.grade,  value, ra, rp, invoice,
type, net, pkgs, kgs, sale_price, buyer_package, b.maxval, b.minval, brokers.name AS brokername,
DATE('1899-12-30') + INTERVAL a.manf_date DAY AS manf_date
FROM closing_cat a
LEFT JOIN brokers ON brokers.code = a.broker
LEFT JOIN (SELECT min(sale_price) AS minval, max(sale_price) AS maxval, mark, grade
FROM `closing_cat`
WHERE sale_no = CONCAT(SUBSTRING_INDEX('$P{sale_no}', '-', 1),'-', SUBSTRING_INDEX('$P{sale_no}', '-', -1)-1)
GROUP BY grade, mark) AS b
ON b.grade = a.grade AND b.mark = a.mark
WHERE sale_no = '$P{sale_no}' AND broker = '$P{broker}' AND a.grade IN ('BP1','PF1') AND category = 'Main'
GROUP BY lot]]>
	</queryString>
